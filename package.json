{
  "name": "dxc-echarts-panel",
  "version": "1.0.0",
  "description": "Echarts panel for grafana",
  "main": "src/module.js",
  "scripts": {
    "build": "grunt",
    "build:watch": "onchange './src/**/*.js' './src/**/*.css' './src/**/*.html' './src/plugin.json' -- npm run build",
    "clean": "grunt clean",
    "lint": "eslint --color .",
    "lint:watch": "onchange 'src/**/*.js' -- npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Billiballa/dxc-echarts-panel.git"
  },
  "keywords": [
    "echarts",
    "grafana",
    "plugin",
    "panel"
  ],
  "author": "Billiballa",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "es6-shim": "^0.35.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "grunt": "~1.0.1",
    "grunt-babel": "~7.0.0",
    "grunt-contrib-clean": "~1.1.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-contrib-uglify": "~2.0.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-execute": "~0.2.2",
    "grunt-systemjs-builder": "^1.0.0",
    "load-grunt-tasks": "^3.5.2"
