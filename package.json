{
  "author": "Volkov Labs",
  "description": "Echarts panel for Grafana",
  "devDependencies": {
    "@grafana/data": "^9.0.0",
    "@grafana/toolkit": "^9.0.0",
    "@grafana/ui": "^9.0.0",
    "@types/codemirror": "^5.60.5",
    "@types/echarts": "^4.9.2",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/lodash": "^4.14.165",
    "codemirror": "^5.58.3",
    "echarts": "^5.3.3",
    "echarts-gl": "^2.0.9",
    "echarts-liquidfill": "^3.1.0",
    "echarts-wordcloud": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "lodash": "^4.17.20"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "Apache-2.0",
  "name": "volkovlabs-echarts-panel",
  "scripts": {
    "build": "grafana-toolkit plugin:build --coverage",
    "dev": "grafana-toolkit plugin:dev",
    "format": "prettier --write \"**\"",
    "sign": "grafana-toolkit plugin:sign --rootUrls http://localhost:3000/",
    "start": "docker-compose pull && docker-compose up",
    "stop": "docker-compose down",
    "test": "grafana-toolkit plugin:test",
    "upgrade": "yarn upgrade --latest",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "version": "1.0.0"
}
